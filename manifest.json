{
    "manifest_version": 2,
    "name": "Answers Autofill",
    "description": "This chrome extension autofills webpage questions with previously saved answers",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "contextMenus",
      "storage",
      "tabs",
      "activeTab",
      "<all_urls>",
      "http://127.0.0.1:5000/"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>", "http://127.0.0.1:5000/"],
        "js": ["content.js"]
      }
    ]
}
    
    